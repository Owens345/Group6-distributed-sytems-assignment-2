{
  "title": "Telecom Edge.Core.Cloud Demo",
  "tags": [
    "telecom",
    "demo"
  ],
  "timezone": "",
  "schemaVersion": 39,
  "version": 1,
  "refresh": "2s",
  "panels": [
    {
      "type": "timeseries",
      "title": "Request rate by service",
      "datasource": {
        "type": "prometheus",
        "uid": "Prometheus"
      },
      "targets": [
        {
          "expr": "sum(rate(service_requests_total[30s])) by (service)",
          "legendFormat": "{{service}}"
        }
      ],
      "gridPos": {
        "x": 0,
        "y": 0,
        "w": 12,
        "h": 8
      }
    },
    {
      "type": "timeseries",
      "title": "Session Manager p95 latency",
      "datasource": {
        "type": "prometheus",
        "uid": "Prometheus"
      },
      "targets": [
        {
          "expr": "histogram_quantile(0.95, sum(rate(service_request_latency_seconds_bucket{service='session_manager'}[30s])) by (le))",
          "legendFormat": "p95"
        }
      ],
      "gridPos": {
        "x": 12,
        "y": 0,
        "w": 12,
        "h": 8
      }
    },
    {
      "type": "timeseries",
      "title": "2PC vs 3PC outcomes",
      "datasource": {
        "type": "prometheus",
        "uid": "Prometheus"
      },
      "targets": [
        {
          "expr": "sum(increase(distributed_transactions_total{service='session_manager'}[1m])) by (protocol,decision)",
          "legendFormat": "{{protocol}} {{decision}}"
        }
      ],
      "gridPos": {
        "x": 0,
        "y": 8,
        "w": 12,
        "h": 8
      }
    },
    {
      "type": "timeseries",
      "title": "Leader election (SM instances)",
      "datasource": {
        "type": "prometheus",
        "uid": "Prometheus"
      },
      "targets": [
        {
          "expr": "service_is_leader{service='session_manager'}",
          "legendFormat": "{{instance_id}}"
        }
      ],
      "gridPos": {
        "x": 12,
        "y": 8,
        "w": 12,
        "h": 8
      }
    },
    {
      "type": "timeseries",
      "title": "Scheduler queue delay p95 (VOICE vs DATA)",
      "datasource": {
        "type": "prometheus",
        "uid": "Prometheus"
      },
      "targets": [
        {
          "expr": "histogram_quantile(0.95, sum(rate(scheduler_queue_delay_seconds_bucket{service='session_manager'}[30s])) by (le,class))",
          "legendFormat": "{{class}} p95"
        }
      ],
      "gridPos": {
        "x": 0,
        "y": 16,
        "w": 12,
        "h": 8
      }
    },
    {
      "type": "timeseries",
      "title": "Deadlocks resolved",
      "datasource": {
        "type": "prometheus",
        "uid": "Prometheus"
      },
      "targets": [
        {
          "expr": "increase(deadlocks_total{service='session_manager'}[5m])",
          "legendFormat": "{{strategy}}"
        }
      ],
      "gridPos": {
        "x": 12,
        "y": 16,
        "w": 12,
        "h": 8
      }
    }
  ]
}